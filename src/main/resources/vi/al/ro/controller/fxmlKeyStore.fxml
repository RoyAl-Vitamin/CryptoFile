<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="15.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tvData" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tcName" editable="false" prefWidth="144.79999083280563" text="Name" />
          <TableColumn fx:id="tcNeedPassword" editable="false" prefWidth="156.80003356933594" text="Need password" />
        </columns>
      </TableView>
      <HBox prefWidth="200.0" VBox.vgrow="NEVER">
         <children>
            <Button mnemonicParsing="false" onAction="#onAddKeyStoreClick" text="Add KeyStore" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
